{
"compilerOptions": {
	"target": "es2022",
	"module": "commonjs",
	"esModuleInterop": true,
	"strict": true,
	"skipLibCheck": true,
	"allowImportingTsExtensions": true,
	"noEmit": true,
	"moduleResolution": "node",
	"types": ["jest", "node"],
	"experimentalDecorators": true,
	"emitDecoratorMetadata": true,
	"sourceMap": true
},
"include": ["src/**/*"]
}
